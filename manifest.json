{
    "name":"seo-tools",
    "version": "1.1",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "browser_action":{
        "default_popup": "popup.html",
        "default_title": "Seo tools"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" 

  
    
}